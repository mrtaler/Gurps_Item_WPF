<Window x:Class="Item_WPF.new_char"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Item_WPF"
        mc:Ignorable="d"
        Title="new_char" Height="300" Width="639.13">
    <Grid x:Name="main1_grid" Margin="0,0,439,0">
        <TextBox x:Name="ST_textBox" HorizontalAlignment="Left" Height="23" Margin="63,10,0,0" TextWrapping="Wrap" Text="{Binding StrengthPoints, Mode=TwoWay}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="DX_textBox" HorizontalAlignment="Left" Height="23" Margin="63,38,0,0" TextWrapping="Wrap" Text="{Binding DexterityPoints, Mode=TwoWay}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="IN_textBox" HorizontalAlignment="Left" Height="23" Margin="63,66,0,0" TextWrapping="Wrap" Text="{Binding IntelligencePoints, Mode=TwoWay}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="HP_textBox" HorizontalAlignment="Left" Height="23" Margin="63,94,0,0" TextWrapping="Wrap" Text="{Binding HealthPoints, Mode=TwoWay}" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Save_button" Content="Button" HorizontalAlignment="Left" Margin="10,239,0,0" VerticalAlignment="Top" Width="75" Click="Save_button_Click"/>

    </Grid>
</Window>
